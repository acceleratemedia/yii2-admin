{
    "name": "brianvb/yii2-admin",
    "description": "Provides a layout and some generic functionality helpful for building admin areas",
    "type": "yii2-extension",
    "require": {
        "brianvb/yii2-crud": "@dev",
        "brianvb/yii2-user": "@dev",
        "kartik-v/yii2-widget-activeform": "@dev",
        "yiisoft/yii2": "^2.0"
    },
    "authors": [
        {
            "name": "BrianVB",
            "email": "vanburen.brian@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
